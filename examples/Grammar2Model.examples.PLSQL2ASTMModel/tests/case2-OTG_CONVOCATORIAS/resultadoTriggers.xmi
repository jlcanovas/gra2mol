    <fragments xsi:type="sastm:RDBTrigger">
      <identifierName nameString="WHEN-MOUSE-DOUBLECLICK"/>
      <body xsi:type="gastm:IfStatement">
        <condition xsi:type="gastm:BinaryExpression">
          <operator xsi:type="gastm:Equal"/>
          <leftOperand xsi:type="gastm:FunctionCallExpression">
            <calledFunction xsi:type="gastm:IdentifierReference">
              <name nameString="GET_ITEM_PROPERTY"/>
            </calledFunction>
            <actualParams xsi:type="gastm:ActualParameterExpression">
              <value xsi:type="gastm:IdentifierReference">
                <name nameString=":SYSTEM.MOUSE_ITEM"/>
              </value>
            </actualParams>
            <actualParams xsi:type="gastm:ActualParameterExpression">
              <value xsi:type="gastm:FunctionCallExpression">
                <calledFunction xsi:type="gastm:IdentifierReference">
                  <name nameString="ITEM_TYPE"/>
                </calledFunction>
              </value>
            </actualParams>
          </leftOperand>
          <rightOperand xsi:type="gastm:StringLiteral" value="'TEXT ITEM' "/>
        </condition>
        <thenBody xsi:type="gastm:IfStatement">
          <condition xsi:type="gastm:BinaryExpression">
            <operator xsi:type="gastm:Or"/>
            <leftOperand xsi:type="gastm:BinaryExpression">
              <operator xsi:type="gastm:Equal"/>
              <leftOperand xsi:type="gastm:FunctionCallExpression">
                <calledFunction xsi:type="gastm:IdentifierReference">
                  <name nameString="GET_ITEM_PROPERTY"/>
                </calledFunction>
                <actualParams xsi:type="gastm:ActualParameterExpression">
                  <value xsi:type="gastm:IdentifierReference">
                    <name nameString=":SYSTEM.CURSOR_ITEM"/>
                  </value>
                </actualParams>
                <actualParams xsi:type="gastm:ActualParameterExpression">
                  <value xsi:type="gastm:FunctionCallExpression">
                    <calledFunction xsi:type="gastm:IdentifierReference">
                      <name nameString="LIST"/>
                    </calledFunction>
                  </value>
                </actualParams>
              </leftOperand>
              <rightOperand xsi:type="gastm:StringLiteral" value="'TRUE' "/>
            </leftOperand>
            <rightOperand xsi:type="gastm:BinaryExpression">
              <operator xsi:type="gastm:Equal"/>
              <leftOperand xsi:type="gastm:FunctionCallExpression">
                <calledFunction xsi:type="gastm:IdentifierReference">
                  <name nameString="GET_ITEM_PROPERTY"/>
                </calledFunction>
                <actualParams xsi:type="gastm:ActualParameterExpression">
                  <value xsi:type="gastm:IdentifierReference">
                    <name nameString=":SYSTEM.CURSOR_ITEM"/>
                  </value>
                </actualParams>
                <actualParams xsi:type="gastm:ActualParameterExpression">
                  <value xsi:type="gastm:FunctionCallExpression">
                    <calledFunction xsi:type="gastm:IdentifierReference">
                      <name nameString="DATATYPE"/>
                    </calledFunction>
                  </value>
                </actualParams>
              </leftOperand>
              <rightOperand xsi:type="gastm:StringLiteral" value="'DATE' "/>
            </rightOperand>
          </condition>
          <thenBody xsi:type="gastm:ExpressionStatement">
            <expression xsi:type="gastm:FunctionCallExpression">
              <calledFunction xsi:type="gastm:IdentifierReference">
                <name nameString="EXECUTE_TRIGGER"/>
              </calledFunction>
              <actualParams xsi:type="gastm:ActualParameterExpression">
                <value xsi:type="gastm:StringLiteral" value="'KEY-LISTVAL' "/>
              </actualParams>
            </expression>
          </thenBody>
        </thenBody>
      </body>
