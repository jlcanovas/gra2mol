    <fragments xsi:type="sastm:RDBTrigger">
      <identifierName nameString="KEY-LISTVAL"/>
      <body xsi:type="gastm:BlockStatement">
        <subStatements xsi:type="gastm:IfStatement">
          <condition xsi:type="gastm:BinaryExpression">
            <operator xsi:type="gastm:Equal"/>
            <leftOperand xsi:type="gastm:FunctionCallExpression">
              <calledFunction xsi:type="gastm:IdentifierReference">
                <name nameString="GET_ITEM_PROPERTY"/>
              </calledFunction>
              <actualParams xsi:type="gastm:ActualParameterExpression"/>
              <actualParams xsi:type="gastm:ActualParameterExpression">
                <value xsi:type="gastm:FunctionCallExpression">
                  <calledFunction xsi:type="gastm:IdentifierReference">
                    <name nameString="DATATYPE"/>
                  </calledFunction>
                </value>
              </actualParams>
            </leftOperand>
            <rightOperand xsi:type="gastm:StringLiteral" value="'DATE' "/>
          </condition>
          <thenBody xsi:type="gastm:IfStatement">
            <condition xsi:type="gastm:BinaryExpression">
              <operator xsi:type="gastm:Equal"/>
              <leftOperand xsi:type="gastm:IdentifierReference">
                <name nameString=":SYSTEM.CURRENT_VALUE"/>
              </leftOperand>
              <rightOperand xsi:type="gastm:StringLiteral" value="null"/>
            </condition>
            <thenBody xsi:type="gastm:ExpressionStatement">
              <expression xsi:type="gastm:BinaryExpression">
                <operator xsi:type="gastm:OperatorAssign"/>
                <leftOperand xsi:type="gastm:IdentifierReference">
                  <name nameString="finicio"/>
                </leftOperand>
                <rightOperand xsi:type="gastm:FunctionCallExpression">
                  <calledFunction xsi:type="gastm:IdentifierReference">
                    <name nameString="to_date"/>
                  </calledFunction>
                  <actualParams xsi:type="gastm:ActualParameterExpression">
                    <value xsi:type="gastm:IdentifierReference">
                      <name nameString=":SYSTEM.CURRENT_VALUE"/>
                    </value>
                  </actualParams>
                  <actualParams xsi:type="gastm:ActualParameterExpression">
                    <value xsi:type="gastm:StringLiteral" value="'DD-MON-YYYY' "/>
                  </actualParams>
                </rightOperand>
              </expression>
            </thenBody>
            <elseBody xsi:type="gastm:ExpressionStatement">
              <expression xsi:type="gastm:BinaryExpression">
                <operator xsi:type="gastm:OperatorAssign"/>
                <leftOperand xsi:type="gastm:IdentifierReference">
                  <name nameString="finicio"/>
                </leftOperand>
                <rightOperand xsi:type="gastm:FunctionCallExpression">
                  <calledFunction xsi:type="gastm:IdentifierReference">
                    <name nameString="sysdate"/>
                  </calledFunction>
                </rightOperand>
              </expression>
            </elseBody>
          </thenBody>
          <elseBody xsi:type="gastm:ExpressionStatement">
            <expression xsi:type="gastm:FunctionCallExpression">
              <calledFunction xsi:type="gastm:IdentifierReference">
                <name nameString="LIST_VALUES"/>
              </calledFunction>
            </expression>
          </elseBody>
        </subStatements>
      </body>
    </fragments>
